<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.sistemacaixa.ProductController"
            prefHeight="400.0" prefWidth="600.0">

    <Label>Verificador de Preço</Label>
    <Label>Código do Produto:</Label>
    <TextField fx:id="TextProduto" onKeyTyped="#onTextProdutoKeyTyped"/>
    <Label>Descrição</Label>
    <Label fx:id="LabelDescricaoProduto"/>
    <Label>Valor:</Label>
    <Label fx:id="LabelPreco"/>

</VBox>
